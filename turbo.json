{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "cache": false
    },
    "lint": {
      "cache": false
    }
  }
}
